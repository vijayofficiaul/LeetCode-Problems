// Problem 217. Contains Duplicate
// Link: https://leetcode.com/problems/contains-duplicate?envType=problem-list-v2&envId=vmgzc3ut


class Solution {
    public boolean containsDuplicate(int[] nums) {
    	
      
 HashSet<Integer> seen = new HashSet<>();
        
        for (int num : nums) {
            if (seen.contains(num)) {
                return true; 
            }
            seen.add(num);
        }
        
        return false; 

    }
}
